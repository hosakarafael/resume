// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("PRISMA_DATABASE_URL")
}

model user {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  firstName String
  lastName String 
  email String @unique
  fileName String?
}